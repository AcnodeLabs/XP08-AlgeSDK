# cp -f ../../../../SANDBOX/Xcode_output/libfmod* /usr/local/lib | grep pp:
#ios-deploy --debug --bundle_id 'com.acnodelabs.apps.alge.AlgeApp' --bundle `mdfind AlgeApp.app | grep 'Products'`
